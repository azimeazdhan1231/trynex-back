
{
  "name": "trynex-backend",
  "version": "1.0.0",
  "description": "TryneX Gift - Backend API",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "tsc",
    "start": "node dist/server/index.js",
    "postbuild": "npm run db:migrate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/server/run-migration.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/server/run-seed.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "postgres": "^3.4.3",
    "zod": "^3.22.4",
    "drizzle-kit": "^0.18.1",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0"
  }
}
